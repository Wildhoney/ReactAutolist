{
    "name": "react-autolist",
    "version": "0.1.1",
    "description": "Browser native implementation of autocomplete using the datalist element.",
    "main": "dist/react-autolist.cjs.js",
    "module": "dist/react-autolist.esm.js",
    "repository": "git@github.com:Wildhoney/ReactAutolist.git",
    "author": "Adam Timberlake <adam.timberlake@gmail.com>",
    "license": "MIT",
    "scripts": {
        "build": "rollup -c && next build ./example",
        "start": "next start ./example -p ${PORT:-3000}",
        "dev": "next ./example -p ${PORT:-3000}",
        "format": "prettier --write '**/**.{js,json}'",
        "test": "yarn lint && yarn spec",
        "spec": "ava",
        "lint": "eslint '**/**.js'",
        "coverage": "nyc --reporter=text-lcov ava | coveralls",
        "deploy": "vercel"
    },
    "dependencies": {
        "prop-types": "^15.7.2",
        "ramda": "^0.27.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-use": "^15.3.2"
    },
    "devDependencies": {
        "@babel/polyfill": "^7.10.4",
        "@babel/register": "^7.10.5",
        "@emotion/core": "^10.0.28",
        "@emotion/styled": "^10.0.27",
        "ava": "^3.10.1",
        "babel-eslint": "^10.1.0",
        "babel-plugin-emotion": "^10.0.33",
        "browser-env": "^3.3.0",
        "coveralls": "^3.1.0",
        "delay": "^4.4.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "eslint": "^7.5.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.20.0",
        "eslint-plugin-standard": "^4.0.1",
        "next": "^9.4.4",
        "nyc": "^15.1.0",
        "prettier": "^2.0.5",
        "rollup": "^2.22.1",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-jsx": "^1.0.3",
        "rollup-plugin-terser": "^6.1.0",
        "sinon": "^9.0.2"
    }
}
